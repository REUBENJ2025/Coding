# First function to check age, returns values,
def Age_Input():
    global input_age
    input_age = int(input("Enter your age: "))  
        

# [comment]
def Age_Check():
    if input_age >= 0 and input_age <= 100:
        print("Valid age")
    else:
        print("Invalid age")

Age_Input()
Age_Check()